{
  "name": "@reverb-app/cli",
  "version": "0.1.0",
  "description": "Log explorer for Reverb",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "contributors": [
    "Christopher Perkins",
    "Blake Fromkin",
    "Morgan Davis",
    "Vincent Sijssens"
  ],
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "reverb-cli": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "reverb-cli",
    "commands": "./dist/commands",
    "dirname": "reverb-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": ":",
    "topics": {
      "events": {
        "description": "Commands related to events"
      },
      "functions": {
        "description": "Commands related to functions"
      },
      "api": {
        "description": "Commands for the configuration of api settings"
      },
      "cdk": {
        "description": "Commands to deploy/destroy reverb infrastructure"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^4.1.2",
    "decompress": "^4.2.1",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/decompress": "^4.2.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.5.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
